{
  "name": "vdhoangson/laravel-repository",
  "description": "Repository pattern package for Laravel framework",
  "type": "library",
  "version": "1.1.3",
  "keywords": [
    "vdhoangson",
    "laravel-repository",
    "repository pattern",
    "repository",
    "framework",
    "laravel"
  ],
  "require": {
    "php": "^8.0|^8.1",
    "ext-json": "*",
    "ext-libxml": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "vdhoangson",
      "email": "vdhoangson@gmail.com",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "Vdhoangson\\LaravelRepository\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Vdhoangson\\LaravelRepository\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Vdhoangson\\LaravelRepository\\Providers\\LaravelRepositoryServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable"
}
