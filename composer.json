{
  "name": "vdhoangson/laravel-repository",
  "description": "Repository pattern package for Laravel framework",
  "type": "library",
  "version": "2.0.0",
  "keywords": [
    "vdhoangson",
    "laravel-repository",
    "repository pattern",
    "repository",
    "framework",
    "laravel",
    "laravel 11",
    "php 8.4"
  ],
  "require": {
    "php": "^8.4",
    "laravel/framework": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0|^11.0"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "vdhoangson",
      "email": "vdhoangson@gmail.com",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "Vdhoangson\\LaravelRepository\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Vdhoangson\\LaravelRepository\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
    "analyse": "vendor/bin/phpstan analyse --memory-limit=2G",
    "format": "vendor/bin/php-cs-fixer fix"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Vdhoangson\\LaravelRepository\\LaravelRepositoryServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable"
}
